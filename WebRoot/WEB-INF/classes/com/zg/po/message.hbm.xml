<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.zg.po.Message" table="message">
		<id name="id">
			<generator class="native">
			</generator>
		</id>
		<property name="userid">
			<column name="userid"></column>
		</property>
		<property name="bookid">
			<column name="bookid"></column>
		</property>
		<property name="content">
			<column name="content"></column>
		</property>
		<property name="createtime">
			<column name="createtime"></column>
		</property>
		<property name="userView">
			<column name="userView"></column>
		</property>
		<property name="bookView">
			<column name="bookView"></column>
		</property>
	</class>
</hibernate-mapping>
