<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.zg.po.Chapter" table="chapter">
		<id name="id">
			<generator class="native">
			</generator>
		</id>
		<property name="bookid">
			<column name="bookid"></column>
		</property>
		<property name="index">
			<column name="index_"></column>
		</property>
		<property name="location">
			<column name="location"></column>
		</property>
		<property name="cost">
			<column name="cost"></column>
		</property>
		<property name="title">
			<column name="title"></column>
		</property>
	</class>
</hibernate-mapping>
